    "autoload-dev": {
        "psr-4": {
            "Laravelfy\\Commentable\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laravelfy\\Commentable\\ServiceProviders\\ServiceProvider"
            ]
        }
    }
}
