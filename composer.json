{
    "name": "laravelfy/commentable",
    "description": "Laravel 快速实现评论的功能",
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "xiaohui lam",
        "email": "xiaohui.lam@icloud.com"
    }],
    "require": {
        "laravel/framework": ">5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0",
        "fzaninotto/faker": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Laravelfy\\Commentable\\": "src/"
        }
