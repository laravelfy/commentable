{
    "name": "laravelfy/laravel-commentable",
    "description": "Laravel 快速实现评论的功能",
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "xiaohui.lam",
        "email": "xiaohui.lam@icloud.com"
    }],
    "require": {
        "laravel/framework": "5.7.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Laravelfy\\Commentable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravelfy\\Commentable\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Laravelfy\\Commentable\\ServiceProvider"
            ]
        }
    }
}
